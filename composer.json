{
    "name": "Azak1r/Dscan",
    "description": "A directional scan (Dscan) result sharing tool for EVE Online. This tool is part of the Leviathan Suite",
    "keywords": ["laravel", "Leviathan", "EVE Online", "Dscan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joery Pigmans",
            "email": "dj-joer@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "laravelcollective/html": "^5.7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azak1r\\Dscan\\DscanServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Azak1r\\Dscan\\": "src/"
        },
        "files": [
            "Azak1r\\Dscan\\Tools\\Static_data.php"
        ]
    }
}
